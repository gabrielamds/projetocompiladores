/* teste_simples.cm - Programa simples para teste do Compilador C- */

/* Variavel global */
int x;

/* Funcao soma */
int soma(int a, int b) {
    int resultado;
    resultado = a + b;
    return resultado;
}

/* Funcao principal */
int main(void) {
    int a;
    int b;
    
    a = 5;
    b = 10;
    x = a + b;
    
    if (x > 10) {
        x = x - 1;
    } else {
        x = x + 1;
    }
    
    while (x > 0) {
        x = x - 1;
    }
    
    return 0;
}
